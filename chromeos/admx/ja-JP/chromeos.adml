  &quot;time_window_limit&quot;: {
    &quot;entries&quot;: [
      {
        &quot;starts_at&quot;: {
          &quot;hour&quot;: 21, 
          &quot;minute&quot;: 0
        }, 
        &quot;effective_day&quot;: &quot;WEDNESDAY&quot;, 
        &quot;last_updated_millis&quot;: &quot;1000000&quot;, 
        &quot;ends_at&quot;: {
          &quot;hour&quot;: 7, 
          &quot;minute&quot;: 30
        }
      }
