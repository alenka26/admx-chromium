{
  &quot;activity_reporting_enabled&quot;: false, 
  &quot;app_limits&quot;: [
    {
      &quot;app_info&quot;: {
        &quot;app_type&quot;: &quot;ARC&quot;, 
        &quot;app_id&quot;: &quot;com.example.myapp&quot;
