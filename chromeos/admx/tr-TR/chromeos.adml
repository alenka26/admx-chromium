      &quot;action_specific_data&quot;: {
        &quot;duration_mins&quot;: 30
      }, 
      &quot;created_at_millis&quot;: &quot;1250000&quot;
    }
  ], 
  &quot;time_window_limit&quot;: {
    &quot;entries&quot;: [
      {
        &quot;starts_at&quot;: {
          &quot;hour&quot;: 21, 
